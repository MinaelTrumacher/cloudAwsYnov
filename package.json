{
  "name": "aws-labs-repository",
  "version": "1.3.0",
  "description": "Educational AWS labs repository for Master 2 students to learn AWS services through hands-on exercises",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "validate-setup": "node shared/validate-setup.js",
    "validate-sso": "node shared/validate-setup.js --sso",
    "prepare": "husky install"
  },
  "keywords": [
    "aws",
    "education",
    "labs",
    "typescript",
    "cloud-computing",
    "learning"
  ],
  "author": "AWS Labs Educational Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-api-gateway": "^3.490.0",
    "@aws-sdk/client-cloudformation": "^3.490.0",
    "@aws-sdk/client-dynamodb": "^3.490.0",
    "@aws-sdk/client-ec2": "^3.490.0",
    "@aws-sdk/client-ecs": "^3.490.0",
    "@aws-sdk/client-iam": "^3.490.0",
    "@aws-sdk/client-iot": "^3.490.0",
    "@aws-sdk/client-lambda": "^3.490.0",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/client-sts": "^3.490.0",
    "@aws-sdk/credential-providers": "^3.490.0",
    "@aws-sdk/util-dynamodb": "^3.490.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "fast-check": "^3.15.0",
    "husky": "^8.0.3",
    "jest": "^25.0.0",
    "prettier": "^3.1.0",
    "ts-jest": "^27.0.3",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}
